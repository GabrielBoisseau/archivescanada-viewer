{
  "name": "lac-harmonized-viewer",
  "version": "1.0.0",
  "description": "Open-source web-based viewer for archival material. IIIF compliant & based on JQuery and Openseadragon.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "scripts": {
    "build": "stencil build",
    "build.docs": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "start.es5": "node --max-old-space-size=4096 ./node_modules/@stencil/core/bin/stencil build --dev --es5 --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "copy.demo": "cpy src/demo.html demo --rename=index.html && cpy src/manifest.json demo && cpy dist/**/*.* demo --parents"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bac-lac/lac-harmonized-viewer.git"
  },
  "keywords": [
    "viewer",
    "archives",
    "iiif",
    "openseadragon",
    "pdf"
  ],
  "author": "Government of Canada",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bac-lac/lac-harmonized-viewer/issues"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "homepage": "https://github.com/bac-lac/lac-harmonized-viewer#readme",
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@material/dialog": "^4.0.0",
    "@mdi/font": "^4.5.95",
    "@stencil/redux": "^0.1.2",
    "@types/mime": "^2.0.1",
    "animate.css": "^3.7.2",
    "axios": "^0.19.0",
    "bulma": "^0.8.0",
    "content-type-parser": "^1.0.2",
    "i18next": "^17.0.16",
    "i18next-browser-languagedetector": "^3.1.1",
    "i18next-chained-backend": "^2.0.0",
    "i18next-localstorage-backend": "^3.0.0",
    "i18next-xhr-backend": "^3.2.2",
    "intersection-observer": "^0.7.0",
    "manifesto.js": "^3.0.12",
    "material-components-web": "^4.0.0",
    "material-design-icons": "^3.0.1",
    "mime": "^2.4.4",
    "nouislider": "^14.0.3",
    "openseadragon": "^2.4.1",
    "overlayscrollbars": "^1.10.0",
    "popper.js": "^1.16.0",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "tippy.js": "^5.1.1",
    "tooltip.js": "^1.3.3",
    "ts-md5": "^1.2.7"
  },
  "devDependencies": {
    "@stencil/core": "1.8.1",
    "@stencil/sass": "^1.0.1",
    "@types/node": "^12.7.4",
    "@types/nouislider": "^9.0.5",
    "@types/overlayscrollbars": "^1.9.0",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "cpy-cli": "^3.1.1",
    "css-loader": "^3.2.0",
    "extract-loader": "^3.1.0",
    "file-loader": "^4.2.0",
    "node-sass": "^8.0.0",
    "postcss": "^7.0.18",
    "postcss-loader": "^3.0.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "sass-loader": "^8.0.0",
    "stencil-inline-svg": "^1.0.1",
    "ts-loader": "^6.0.4",
    "typescript": "^3.7.2",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.3",
    "webpack-stream": "^5.2.1"
  }
}
